                            android:layout_marginLeft="15dp"
                            android:layout_marginBottom="5dp"
                            android:text="激活导管室"
                            android:textSize="15sp"
                            android:textColor="#131D35"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    <TextView
