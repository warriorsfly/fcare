<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--设置进度条的背景-->
    <item android:id="@android:id/background" android:drawable="@drawable/bg_conor_voice_red">
    </item>
    <!--设置进度条的缓冲进度-->
    <item android:id="@android:id/secondaryProgress" android:drawable="@drawable/bg_conor_light_blue">
    </item>
    <!--设置进度条的真实进度-->
    <item android:id="@android:id/progress" android:drawable="@drawable/bg_conor_light_blue">
    </item>

</layer-list>
